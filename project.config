[receive]
	requireChangeId = true
[access "refs/*"]
	create = group chef-manager-core
[access "refs/heads/*"]
	abandon = group chef-manager-core
	label-Code-Review = -2..+2 group chef-manager-core
	label-Workflow = -1..+1 group chef-manager-core
[access "refs/heads/stable/*"]
	abandon = group chef-manager-core
	label-Code-Review = -2..+2 group chef-manager-core
	label-Workflow = -1..+1 group chef-manager-core
